#include <stdio.h>

#define SIZE 100

int main()
{

	int i = 1;
	int cnt = 1;
	int percent = 0;

	for(i;i<=100;i++){
		percent = (i*100)/SIZE; // *100;
		
//		printf("  percent check : %d ", percent);
		if(percent/cnt !=0 &&  percent%cnt ==0){
			printf("percentage : %d percent\n",percent);
			cnt ++;
		}		
	}

	return 0;
	
}
