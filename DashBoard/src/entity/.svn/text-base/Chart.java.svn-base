package entity;

public class Chart {

	private int rowCount;
	private String xCategories;
	private String name1, name2, name3;
	private String dataOpen, dataMerge, dataAbandon;
	
	/* ****** Constructor ****** */
	public Chart(){
		
	}
	
	public Chart(int rowCount, String xCategories, String name1, String name2, String name3, String dataOpen, String dataMerge, String dataAbandon){
		super();
		this.rowCount = rowCount;
		this.xCategories = xCategories;
		this.name1 = name1;
		this.name2 = name2;
		this.name3 = name3;
		this.dataOpen = dataOpen;
		this.dataMerge = dataMerge;
		this.dataAbandon = dataAbandon;
	}
	
	/* ****** method ****** */
	public int getRowCount()
	{
		return rowCount;
	}
	
	public String getXCategories(){
		return xCategories;
	}
	
	public String getName1(){
		return name1;
	}
	
	public String getName2(){
		return name2;
	}

	public String getName3(){
		return name3;
	}
	
	public String getDataOpen()
	{
		return dataOpen;
	}

	public String getDataMerge()
	{
		return dataMerge;
	}
	
	public String getDataAbandon()
	{
		return dataAbandon;
	}
}
